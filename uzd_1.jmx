          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils

String firstname = RandomStringUtils.randomAlphabetic(6)
String lastname = RandomStringUtils.randomAlphabetic(8)

int totalprice = (50..500).toList().get(new Random().nextInt(451))

vars.put(&quot;firstname&quot;, firstname)
vars.put(&quot;lastname&quot;, lastname)
vars.put(&quot;totalprice&quot;, totalprice.toString())

def today = new Date()
def checkin = today.format(&apos;yyyy-MM-dd&apos;)
def checkout = (today.plus(7)).format(&apos;yyyy-MM-dd&apos;)
vars.put(&quot;checkin&quot;, checkin)
vars.put(&quot;checkout&quot;, checkout)

vars.put(&quot;additionalneeds&quot;, &quot;Breakfast&quot;)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
